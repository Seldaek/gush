{
    "name": "cordoval/gush",
    "description": "Rapid workflow for Github project maintainers and contributors",
    "license": "MIT",
    "authors": [
        {
            "name": "cordoval",
            "email": "cordoval@gmail.com"
        },
        {
            "name": "Daniel Gomes",
            "email": "me@danielcsgomes.com"
        },
        {
            "name": "Daniel T. Leech",
            "email": "dan.t.leech@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "lib-curl": "*",
        "symfony/console": "~2.3",
        "symfony/process": "~2.3",
        "symfony/yaml": "~2.3",
        "knplabs/github-api": "~1.2",
        "ddd/slug": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "raulfraile/ladybug": "~1.0",
        "behat/behat": "~2.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Gush\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
