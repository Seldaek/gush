    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "raulfraile/ladybug": "~1.0"
    },
    "bin": [
        "bin/gsh"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {"Gush\\": "src"}
